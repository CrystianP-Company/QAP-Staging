{
  "version": 3,
  "name": "susu-api",
  "timeout": "5000ms",
  "cache_ttl": "300s",
  "output_encoding": "no-op",
  "port": 3890,
  "plugin": {
    "pattern": ".so",
    "folder": "/etc/krakend/plugins/"
  },
  "endpoints": [
    {
      "endpoint": "/susu-api/pet/{petId}",
      "method": "DELETE",
      "output_encoding": "no-op",
      "input_headers": [
        "*"
      ],
      "input_query_strings": [],
      "extra_config": {
        "security/cors": {
          "name": "{petId}",
          "allow_origins": [
            "*"
          ],
          "allow_methods": [
            "GET",
            "HEAD",
            "POST"
          ],
          "expose_headers": [
            "Content-Length",
            "Content-Type"
          ],
          "allow_headers": [
            "Accept-Language"
          ],
          "max_age": "12h",
          "allow_credentials": false,
          "debug": false,
          "url_pattern": "/api/v3/pet/{petId}",
          "encoding": "no-op",
          "sd": "static",
          "timeout": "3000ms",
          "cache_ttl": "300s",
          "disable_host_sanitize": false
        }
      },
      "backend": [
        {
          "url_pattern": "/api/v3/pet/{petId}",
          "encoding": "no-op",
          "sd": "static",
          "method": "DELETE",
          "extra_config": {},
          "host": [
            "https://petstore3.swagger.io"
          ],
          "cache_ttl": "300s"
        }
      ]
    },
    {
      "endpoint": "/susu-api/pet/{petId}",
      "method": "GET",
      "output_encoding": "no-op",
      "input_headers": [
        "*"
      ],
      "input_query_strings": [],
      "extra_config": {
        "security/cors": {
          "name": "{petId}",
          "allow_origins": [
            "*"
          ],
          "allow_methods": [
            "GET",
            "HEAD",
            "POST"
          ],
          "expose_headers": [
            "Content-Length",
            "Content-Type"
          ],
          "allow_headers": [
            "Accept-Language"
          ],
          "max_age": "12h",
          "allow_credentials": false,
          "debug": false,
          "url_pattern": "/api/v3/pet/{petId}",
          "encoding": "no-op",
          "sd": "static",
          "timeout": "3000ms",
          "cache_ttl": "300s",
          "disable_host_sanitize": false
        }
      },
      "backend": [
        {
          "url_pattern": "/api/v3/pet/{petId}",
          "encoding": "no-op",
          "sd": "static",
          "method": "GET",
          "extra_config": {},
          "host": [
            "https://petstore3.swagger.io"
          ],
          "cache_ttl": "300s"
        }
      ]
    }
  ],
  "extra_config": {
    "plugin/http-server": {
      "name": [
        "krakend-server-billing"
      ],
      "krakend-server-billing": {
        "endpoint_checker": "http://billing-request-checker:8091/check",
        "endpoint_charge": "http://billing-request-charger:8092/charge"
      }
    }
  }
}